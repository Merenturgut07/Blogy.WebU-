@model BlogSendContact


@{
	ViewData["Title"] = "DetailsContact";
	Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<br />
<div class="page-header">
	<div class="main-panel">
		<div class="col-lg-12 grid-margin stretch-card">
			<div class="card">
				<div class="card-body">
					<h4 class="card-title"> Mesajların Detayları </h4>
					<br />
					<form class="form-group">
						@Html.HiddenFor(x => x.Id, new { @class = "form-control" })
						@Html.Label("Mesaj")
						@Html.TextAreaFor(x => x.Message, 10, 2, new { @class = "form-control" })
						<br />
					</form>
				</div>
			</div>
		</div>
	</div>
</div>



